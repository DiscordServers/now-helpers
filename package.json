{
    "name": "@discordservers/now-helpers",
    "description": "Helpers for the DiscordServers zeit functions",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "publish-dev": "rm dist/* && npm run build && npm publish . --tag dev",
        "build": "tsc --project tsconfig.json && ts-node tools/after-build.ts",
        "lint": "tslint --config tslint.json --project tsconfig.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            "@semantic-release/git",
            "@semantic-release/npm"
        ]
    },
    "dependencies": {
        "@secretary/aws-secrets-manager": "^1.0.1-alpha.9",
        "@secretary/core": "^1.0.1-alpha.9",
        "@sentry/node": "^4.6.5",
        "dotenv": "^8.0.0",
        "ip-range-check": "^0.2.0",
        "micro": "^9.3.3",
        "micro-cors": "^0.1.1",
        "micro-query": "^0.3.0",
        "micro-redirect": "^1.0.0",
        "prom-client": "^11.5.1",
        "redis": "^2.8.0",
        "request": "^2.88.0",
        "request-ip": "^2.1.3",
        "request-promise": "^4.2.4",
        "source-map-support": "^0.5.11"
    },
    "devDependencies": {
        "@types/micro": "^7.3.3",
        "@types/micro-cors": "^0.1.0",
        "@types/node": "^11.11.6",
        "@types/redis": "^2.8.11",
        "@types/request": "^2.48.1",
        "@types/request-ip": "0.0.33",
        "@types/request-promise": "^4.1.42",
        "@types/source-map-support": "^0.5.0",
        "aws-sdk": "^2.427.0",
        "axios": "^0.21.1",
        "ts-node": "^8.3.0",
        "tslint": "^5.14.0",
        "typescript": "^3.3.4000"
    }
}
